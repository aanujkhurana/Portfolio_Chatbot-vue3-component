{
  "name": "vue-portfolio-chatbot",
  "version": "1.0.0",
  "description": "A friendly, reusable chatbot for portfolio websites powered by OpenAI and Vue 3.",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build --lib --name ChatAssistant --entry src/index.ts",
    "preview": "vite preview"
  },
  "keywords": [
    "vue",
    "vue3",
    "chatbot",
    "portfolio",
    "openai",
    "component",
    "library"
  ],
  "author": "Anuj Khurana <https://github.com/anujkhurana>",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "typescript": "^5.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "vue": "^3.4.0"
  }
}
